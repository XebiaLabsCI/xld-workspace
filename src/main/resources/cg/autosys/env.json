<#-- BEGIN FREEMARKER TEMPLATE DEFINITIONS -->

<#assign container = deployed.container>

<#-- END FREEMMARKER TEMPLATE DEFINITIONS -->

{
        "${container.environmentName}": {
                "config": {
                        "machine": "${deployed.applicationName}_Vmachine",
                        "envPrefix": "${container.environmentName}_",
                        "jobOwner": "${container.autosysJobAccount}",
                        "jobAppName": "${deployed.applicationName}",
                        "profile" : "/users/${deployed.applicationName}/config/${deployed.applicationName}_autosys.profile"
                },
                "vmachines": {
                        "${deployed.applicationName}_Vmachine": {
                                "machine": "${container.host1}",
                                "max_load": "20",
                                "factor": "0.00"
                        }
                        "${deployed.applicationName}_Vmachine2": {
                                "machine": "${container.host2}",
                                "max_load": "20",
                                "factor": "0.00"
                        }
                }
        }
}
